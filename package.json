{
  "name": "flapjs",
  "version": "0.5.0",
  "description": "Flap.js Web App",
  "main": "index.js",
  "browser": "main.js",
  "scripts": {
    "start": "webpack-dev-server --env=development --config webpack.config.js",
    "build": "webpack --env=production --config webpack.config.js",
    "docs": "npm run docs-storybook",
    "test": "npm run lint && npm run jest",
    "docs-storybook": "build-storybook -c .storybook -o \"docs/storybook\"",
    "docs-coverage": "npm run jest -- --coverage",
    "test-commit": "lint-staged",
    "storybook": "start-storybook",
    "stories": "npm run storybook",
    "jest": "jest --config \".jest/config.js\"",
    "jest-update": "npm run jest -- --updateSnapshot",
    "lint": "eslint \"src/\" --ext \".js\" --ext \".jsx\" && stylelint \"src/**/*.css\"",
    "boilerplate": "node ./tools/boilerplate/boilerplate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flapjs/FLAPJS-WebApp.git"
  },
  "author": "Flap.js Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flapjs/FLAPJS-WebApp/issues"
  },
  "homepage": "https://github.com/flapjs/FLAPJS-WebApp#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
